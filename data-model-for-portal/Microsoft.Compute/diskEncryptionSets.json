                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/disk-encryption-set#az-disk-encryption-set-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--disk-encryption-set-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get information of a disk encryption sets",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.diskEncryptionSetName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
